/* pages/me/me.wxss */
page{
  background: #eee;
}
.header{
  width: 100%;
  height: 250rpx;
  background: #8CC24A
}
.header image{
  width: 100rpx;
  height: 100rpx;
  margin-left: 30rpx;
  border-radius: 100rpx;
  vertical-align: middle;
}
.header text{
  margin-left: 15rpx;
  font-weight: bold;
}

.center{
  display: flex;
  flex-direction: column;
}


.boxText{
  background: white;
  margin: 20rpx 0;
  width: 100%;
  height: 80rpx;
  position: relative;
}

.boxText1{
  background: white;
  position: relative;
  border: 0.1rpx #eee dashed;
  width: 100%;
  height: 80rpx;
}
.center image{
  width: 45rpx;
  height: 45rpx;
  margin-left: 30rpx;
  margin-top: 20rpx;
}
.center text{
  font-size: 32rpx;
  margin-left: 20rpx;
  position: absolute;
  top:20rpx;

}